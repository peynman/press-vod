{
    "name": "peynman/larapress-vod",
    "description": "Video On Demand as a product type for Larapress ECommerce",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [{
        "name": "Peyman Abdi",
        "email": "larapress@peynman.ir"
    }],
    "autoload": {
        "psr-4": {
            "Larapress\\VOD\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Larapress\\VOD\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Larapress\\VOD\\Providers\\PackageServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^7.0|^8.0",
        "laravel/framework": "^8.0",
        "pbmedia/laravel-ffmpeg": "^7.0",
        "peynman/larapress-ecommerce": "master",
        "peynman/larapress-fileshare": "master"
    },
    "require-dev": {
        "facade/ignition": "^2.3.6",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^5.0",
        "orchestra/testbench": "^6.2.0",
        "phpunit/phpunit": "^9.0",
        "phpmd/phpmd": "@stable",
        "squizlabs/php_codesniffer": "3.*"
    }
}
